{
  "name": "@activewidgets/html",
  "version": "0.0.6",
  "description": "Datagrid component (html custom elements)",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "**/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "public": "npm run build && rimraf public && parcel build examples/index.html -d public",
    "start": "parcel examples/index.html -d public --open",
    "test": "npm run public && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "0.0.8",
    "@activewidgets/frameworks": "0.0.8",
    "htm": "2.2.1",
    "preact": "10.1.1"
  },
  "devDependencies": {
    "@activewidgets/options": "0.0.8",
    "@activewidgets/puppeteer": "0.0.3",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@rollup/plugin-node-resolve": "^6",
    "parcel": "^1",
    "postcss-import": "^12",
    "rimraf": "^3",
    "rollup": "^1.20",
    "rollup-plugin-babel": "^4",
    "rollup-plugin-postcss": "^2",
    "rollup-plugin-sourcemaps": "^0.4",
    "rollup-plugin-terser": "^5",
    "style-inject": "^0.3"
  },
  "jest": {
    "preset": "@activewidgets/puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/html.git"
  },
  "keywords": [
    "datagrid"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/html/issues"
  },
  "homepage": "https://github.com/activewidgets/html#readme"
}
